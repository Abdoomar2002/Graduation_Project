{
  "expo": {
    "name": "HatleyMobileDelivery",
    "slug": "HatleyMobileDelivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "extra": {
      "API_URL": "https://api.example.com",
      "API_KEY": "your_api_key_here",
      "eas": {
        "projectId": "05a8ff44-cbf3-48d8-8631-eee485afb4b6"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(HatleyMobileUser) to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "assets/images/Logo.png"
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.abdoomar.HatleyMobileDelivery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
